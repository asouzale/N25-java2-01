package br.com.senaisp.bauru.secao03.aula07;

import java.util.Scanner;

public class Exemplo05 {

	public static void main(String[] args) {
		Scanner sc = new Scanner (Exemplo05.class.getResourceAsStream("cadastros.csv"));
		//trocando o token scanner
		sc.useDelimiter(",");
		//imprimir o cabe√ßalho
		for (int i=0;i<4;i++) {
			//cuidado que e print e nao println
			System.out.print(sc.next().replace("\"", "") + "\t");
		}
		System.out.println(); //pulando uma linha
		while (sc.hasNext()) {
			System.out.print (Integer.parseInt(sc.next().replace("\r\n", "")));
			break;
		}
		sc.close();

	}

}
